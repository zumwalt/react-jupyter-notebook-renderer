{
  "name": "react-jupyter-notebook-renderer",
  "version": "0.0.1",
  "description": "A React component for rendering Jupyter Notebooks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run copy-css",
    "copy-css": "cp src/*.css dist/",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "jupyter",
    "notebook",
    "renderer"
  ],
  "author": "Casey Zumwalt",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@jupyterlab/nbformat": "^3.2.9",
    "@radix-ui/react-collapsible": "^1.1.0",
    "dompurify": "^3.1.6",
    "katex": "^0.16.11",
    "lucide-react": "^0.441.0",
    "prism-react-renderer": "^2.4.0",
    "react-markdown": "^9.0.1",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "supports-color": "^9.4.0"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.3.1",
    "react": "^18.3.1",
    "typescript": "^5.4.5"
